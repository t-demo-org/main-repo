                container.removeChild(product);
            }

            var section = document.getElementsByClassName('container-list-tiles')[0];
            if (!section) {
                var section = document.createElement('section');
                section.setAttribute('class', 'container-list-tiles');
                container.appendChild(section);
            }
            else
            {
                section.innerHTML = '';
            }

            searchResults.forEach( product => {
                var productsDiv = document.createElement('div');
                var prodImage = document.createElement('img');
                prodImage.setAttribute('src', product.image);
                productsDiv.appendChild(prodImage);
                var prodName = document.createElement('h3');
                prodName.innerHTML = product.name;
                productsDiv.appendChild(prodName);
                var prodRating = document.createElement('img');
                prodRating.setAttribute('src', product.rating)
                productsDiv.appendChild(prodRating);
                var prodPrice = document.createElement('div');
